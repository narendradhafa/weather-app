<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/image_condition5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="24dp"
    tools:context=".WeatherActivityVolley">

    <TextView
        android:id="@+id/text_windspeed_unit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="mph"
        app:layout_constraintEnd_toEndOf="@+id/text_windspeed"
        app:layout_constraintStart_toStartOf="@+id/text_windspeed"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed" />

    <TextView
        android:id="@+id/text_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="48dp"
        android:text="Malang"
        android:textSize="36sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_temperature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="@+id/text_title_temperature"
        app:layout_constraintStart_toStartOf="@+id/text_title_temperature"
        app:layout_constraintTop_toBottomOf="@+id/text_title_temperature"
        tools:text="29" />

    <TextView
        android:id="@+id/text_windspeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="@+id/text_title_windspeed"
        app:layout_constraintStart_toStartOf="@+id/text_title_windspeed"
        app:layout_constraintTop_toBottomOf="@+id/text_title_windspeed"
        tools:text="12.2" />

    <TextView
        android:id="@+id/text_coordinate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/text_city"
        app:layout_constraintTop_toBottomOf="@+id/text_city"
        tools:text="-8, 112.625" />

    <ImageView
        android:id="@+id/image_condition"
        android:layout_width="192dp"
        android:layout_height="192dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sun" />

    <TextView
        android:id="@+id/text_title_temperature"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temperature"
        app:layout_constraintEnd_toStartOf="@+id/text_title_windspeed"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/text_title_windspeed" />

    <TextView
        android:id="@+id/text_condition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_condition"
        tools:text="Sunny" />

    <TextView
        android:id="@+id/text_title_windspeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Wind Speed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/text_title_temperature"
        app:layout_constraintTop_toBottomOf="@+id/text_condition" />

    <TextView
        android:id="@+id/text_condition1"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast"
        app:layout_constraintStart_toStartOf="@+id/image_forecast"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast" />

    <TextView
        android:id="@+id/text_condition2"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast1"
        app:layout_constraintStart_toStartOf="@+id/image_forecast1"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast1" />

    <TextView
        android:id="@+id/text_condition3"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast2"
        app:layout_constraintStart_toStartOf="@+id/image_forecast2"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast2" />

    <TextView
        android:id="@+id/text_condition5"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast4"
        app:layout_constraintStart_toStartOf="@+id/image_forecast4"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast4" />

    <TextView
        android:id="@+id/text_condition4"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast3"
        app:layout_constraintStart_toStartOf="@+id/image_forecast3"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast3" />

    <TextView
        android:id="@+id/text_condition6"
        android:layout_width="48dp"
        android:layout_height="22dp"
        android:text="Sunny"
        app:layout_constraintEnd_toEndOf="@+id/image_forecast5"
        app:layout_constraintStart_toStartOf="@+id/image_forecast5"
        app:layout_constraintTop_toBottomOf="@+id/image_forecast5" />

    <ImageView
        android:id="@+id/image_forecast5"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_forecast4"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/image_forecast1"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/image_forecast2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_forecast"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/image_forecast2"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/image_forecast3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_forecast1"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/image_forecast3"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/image_forecast4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_forecast2"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/image_forecast4"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/image_forecast5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/image_forecast3"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

    <ImageView
        android:id="@+id/image_forecast"
        android:layout_width="49dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/image_forecast1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_windspeed_unit"
        app:srcCompat="@drawable/sun" />

</androidx.constraintlayout.widget.ConstraintLayout>